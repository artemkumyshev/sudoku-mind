{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,css}' '!**/{node_modules,dist}/**/*'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.css'",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@twa-dev/sdk": "^8.0.2",
    "@uidotdev/usehooks": "^2.4.1",
    "atomic-router": "^0.11.0",
    "atomic-router-react": "^0.10.0",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "effector": "^23.3.0",
    "effector-localstorage": "^1.0.0",
    "effector-react": "^23.3.0",
    "history": "^5.3.0",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "lodash": "^4.17.21",
    "lucide-react": "^0.503.0",
    "patronum": "^2.3.0",
    "react": "^19.0.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-rainfall": "^0.1.533",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@effector/redux-devtools-adapter": "^0.2.0",
    "@eslint/js": "^9.22.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.19.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
